// Do any local configuration here
//

// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";

zone "pablo.example" {
	type master;
	file "/etc/bind/zone.pablo.example";
	allow-transfer {
		192.168.99.3;
	};
};

zone "99.168.192.in-addr.arpa" {
	type master;
	file "/etc/bind/inverse.192.168.99";
	allow-transfer {
		192.168.99.3;
	};
};
