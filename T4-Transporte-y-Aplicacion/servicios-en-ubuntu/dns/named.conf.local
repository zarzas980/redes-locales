//ZONA (DOMINIO) QUE GESTIONA EL DNS Ej: "pablo.com" (con comillas)
zone <CAMBIAR> {
	type master;
	//ARCHIVO DONDE ESTÁ GUARDADA LA ZONA (DOMINIO)
	//CAMBIAR EL NOMBRE POR EL TUYO. DEBERÍA SER ALGO COMO zone.pablo.com
	file "/etc/bind/zone.<CAMBIAR>";
};
